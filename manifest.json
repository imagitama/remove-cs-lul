{
    "name": "Remove CS LUL",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Removes all CS LULs from Twitch chats.",
    "homepage_url": "https://github.com/imagitama/remove-cs-lul",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "https://www.twitch.tv/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.twitch.tv/*"],
            "js": ["js/main.js"],
            "run_at": "document_end"
        }
    ]
}